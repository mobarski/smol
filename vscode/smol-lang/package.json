{
    "name": "smol",
    "displayName": "Smol",
    "description": "Syntax highlighting for Smol language",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.58.0"
    },
    "categories": ["Programming Languages"],
    "contributes": {
        "languages": [{
            "id": "smol",
            "aliases": ["Smol", "smol"],
            "extensions": [".smol"]
        }],
        "grammars": [{
            "language": "smol",
            "scopeName": "source.smol",
            "path": "./syntaxes/smol.tmLanguage.json"
        }]
    }
}
